package trail;

public class C {

	public static void main(String[] args) {
		  int []arr1 = {2,3,4,5,6};
		  int [] arr2= {11,23,12,34};
		  
		  int mergeArray= arr1.length+arr2.length;
		  int resultantArray[]=new int[mergeArray];
		  
		  for (int i = 0; i < arr1.length; i++) {
			resultantArray[i]=arr1[i];
		}
		  for (int i = 0; i < arr2.length; i++) {
			resultantArray[arr1.length+i]=arr2[i];
		}
		  for (int i : resultantArray) {
			System.out.print(i+" ");
		}
	}
}
