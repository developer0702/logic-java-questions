package trail;

public class B {

	public static void main(String[] args) {
		int []arr1= {2,3,4,5,6,7,8};
		int [] arr2= {9,10,11,12,13,14,15};
		
		int merge=arr1.length+arr2.length;
		
		  int resultantArray [] =new int[merge];
		  
		  for (int i = 0; i < arr1.length; i++) {
			resultantArray[i]=arr1[i];
		}
		  
		  for (int i = 0; i < arr2.length; i++) {
			resultantArray[arr1.length+i]=arr2[i];
		}
		  for (int i : resultantArray) {
			System.out.print(i+" ");
		}
	}
}
